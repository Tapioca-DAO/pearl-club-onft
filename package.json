    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "fs": "^0.0.1-security",
    "hardhat": "^2.12.6",
    "hardhat-contract-sizer": "^2.7.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-gas-reporter": "^1.0.9",
